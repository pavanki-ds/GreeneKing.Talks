{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SpeakerRegistrationStandardRules": {
    "ExcludeTechnologies": [ "Cobol", "Punch Cards", "Commodore", "VBScript" ],
    "ExcludeDomains": [ "aol.com", "prodigy.com", "compuserve.com" ],
