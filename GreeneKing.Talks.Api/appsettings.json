      {
        "MinYears": 2,
        "MaxYears": 3,
        "Fee": 250
      },
      {
        "MinYears": 4,
        "MaxYears": 5,
        "Fee": 100
      },
      {
        "MinYears": 6,
        "MaxYears": 9,
        "Fee": 50
      },
      {
        "MinYears": 10,
        "MaxYears": 100,
        "Fee": 0
      }
    ]
  }
